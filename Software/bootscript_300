            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x0C; fErrorMon $?;  i2cset -y $vSi5382Bus $vSi5382Addr 0x08 0x01; fErrorMon $?
            sleep 1
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x05; fErrorMon $?;  i2cset -y $vSi5382Bus $vSi5382Addr 0x14 0x01; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?;  i2cset -y $vSi5382Bus $vSi5382Addr 0x1C 0x01; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x05; fErrorMon $?;  i2cset -y $vSi5382Bus $vSi5382Addr 0x40 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x0B; fErrorMon $?;  i2cset -y $vSi5382Bus $vSi5382Addr 0x24 0xC3; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x0B; fErrorMon $?;  i2cset -y $vSi5382Bus $vSi5382Addr 0x25 0x06; fErrorMon $?
         fi
   fi
}


fStartBanner
fCfgSi5382
fCfgLMK04208
fCfgLMX2594
