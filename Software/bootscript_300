    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x42 0x01 0xF4 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x41 0x00 0x00 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x40 0x13 0x88 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x3F 0x00 0x00 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x3E 0x03 0x22 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x3D 0x00 0xA8 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x3C 0x00 0x00 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x3B 0x00 0x01 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x3A 0x80 0x01 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x39 0x00 0x20 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x38 0x00 0x00 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x37 0x00 0x00 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x36 0x00 0x00 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x35 0x00 0x00 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x34 0x08 0x20 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x33 0x00 0x80 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x32 0x00 0x00 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x31 0x41 0x80 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x30 0x03 0x00 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x2F 0x03 0x00 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x2E 0x07 0xFC i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x2D 0xC0 0xCC i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x2C 0x0C 0x23 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x2B 0x00 0x26 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x2A 0x00 0x00 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x29 0x00 0x00 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x28 0x00 0x00 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x27 0x02 0x60 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x26 0x00 0x00 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x25 0x03 0x04 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x24 0x00 0x27 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x23 0x00 0x04 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x22 0x00 0x00 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x21 0x1E 0x21 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x20 0x03 0x93 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x1F 0x43 0xEC i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x1E 0x31 0x8C i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x1D 0x31 0x8C i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x1C 0x04 0x88 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x1B 0x00 0x02 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x1A 0x0D 0xB0 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x19 0x06 0x24 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x18 0x07 0x1A i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x17 0x00 0x7C i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x16 0x00 0x01 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x15 0x04 0x01 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x14 0xE0 0x48 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x13 0x27 0xB7 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x12 0x00 0x64 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x11 0x01 0x2C i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x10 0x00 0x80 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x0F 0x06 0x4F i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x0E 0x1E 0x70 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x0D 0x40 0x00 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x0C 0x50 0x01 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x0B 0x00 0x18 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x0A 0x10 0xD8 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x09 0x16 0x04 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x08 0x20 0x00 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x07 0x40 0xB2 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x06 0xC8 0x02 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x05 0x00 0xC8 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x04 0x0C 0x43 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x03 0x06 0x42 i
    	i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x02 0x05 0x00 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x01 0x08 0x08 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x1 0x00 0x24 0x9C i
		printf "Configuring LMX2594_A.... done\n"
	fi
}

# Configure Si5382 PLL (for ZCU111 only)
fCfgSi5382 () {
    fFindSi5382

      if [ $vSi5382Found -eq 1 ]; then
         # Use 161 for designs with rev 1.0.x, or for later FHA designs
         if ( [ $vSi5382_clk == "161" ] ); then
            printf "Configuring Si5382 output for 161.132812 MHz operation...\n"
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x0B; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x24 0xC0; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x0B; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x25 0x04; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x05; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x40 0x01; fErrorMon $?
            sleep 1
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x06 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x07 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x08 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x0B 0x68; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x16 0x03; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x17 0xDC; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x18 0xEE; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x19 0xFF; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x1A 0xFF; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x20 0x02; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x2B 0x02; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x2C 0x01; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x2D 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x2E 0x3C; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x2F 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x30 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x31 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x32 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x33 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x34 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x35 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x36 0x02; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x37 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x38 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x39 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x3A 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x3B 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x3C 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x3D 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x3E 0x10; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x3F 0x11; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x40 0x04; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x41 0x0E; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x42 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x43 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x44 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x45 0x0C; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x46 0x32; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x47 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x48 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x49 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x4A 0x32; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x4B 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x4C 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x4D 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x4E 0x05; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x4F 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x50 0x0F; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x51 0x03; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x52 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x53 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x54 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x55 0x03; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x56 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x57 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x58 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x59 0x01; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x5A 0xE3; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x5B 0xF8; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x5C 0xBE; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x5D 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x5E 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x5F 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x60 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x61 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x62 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x63 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x64 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x65 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x66 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x67 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x68 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x69 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x92 0x02; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x93 0xA0; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x95 0x00; fErrorMon $?
            i2cset -y $vSi5382Bus $vSi5382Addr 0x01 0x00; fErrorMon $?; i2cset -y $vSi5382Bus $vSi5382Addr 0x96 0x80; fErrorMon $?
