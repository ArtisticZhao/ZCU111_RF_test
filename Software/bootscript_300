#!/bin/sh

#-----------------------------------------------------------------------
# Configure ZCU111 Reference Clocks
#


#-----------------------------------------------------------------------
# Variables
#-----------------------------------------------------------------------
# common
vScriptName=`basename $0` # Test script name
vPauseOnError=1
vInteractive=0
vRetVal=0
vTotalChecks=0
# custom
vLastRdVal=0
vMode="cfg"
# Si5382 is normally @ bus=14, address=0x68
vSi5382Found=0
vSi5382Bus=99
vSi5382Addr=68
# SCI18IS602 is normally @ bus=14, address=0x2f
vSCI18IS602Found=0
vSCI18IS602Bus=99
vSCI18IS602Addr=2f
# LMK04208 is accessed through the SCI18IS602 I2C-to-SPI bridge using slave select SS1 = 0x02
vLMKAddr=0x02
vSi5382_clk=161

#-----------------------------------------------------------------------
# Functions
#-----------------------------------------------------------------------

# Error monitor
fErrorMon () {
   vTotalChecks=$(( $vTotalChecks + 1 ))
   if [ $1 -gt 0 ]; then
      vRetVal=$(( $vRetVal + 1 ))

      # Pause to review error
      if [ $vPauseOnError -eq 1 ]; then
         echo "Error detected ($vRetVal Errors, $vTotalChecks Checks)!  Hit ENTER to resume..."
         read vDummy
      else
         echo "Error detected ($vRetVal Errors, $vTotalChecks Checks)!"
      fi
   fi
}

# Start banner
fStartBanner () {
   echo ""
   printf "#-----------------------------------------------------------------------\n"
   if [ $vInteractive -eq 1 ]; then
      printf "# $vScriptName: Hit ENTER to start...\n"
      read
   else
      printf "# $vScriptName: STARTED\n"
   fi
   printf "#-----------------------------------------------------------------------\n"
}

# End banner
fEndBanner () {
   printf "#-----------------------------------------------------------------------\n"
   if [ $vRetVal -gt 0 ]; then
      printf "# $vScriptName: FINISHED - FAILED [%d Errors, %d Checks]\n" $vRetVal $vTotalChecks
   else
      printf "# $vScriptName: FINISHED - PASSED [%d Checks]\n" $vTotalChecks
   fi
   printf "#-----------------------------------------------------------------------\n"
   echo ""
   exit $vRetVal
}


# I2C Paged Read with expected value and masking support
fI2cRdPaged () {
   # 1 - bus
   # 2 - device address
   # 3 - page register address
   # 4 - page number
   # 5 - register address
   # 6 - expected read value
   # 7 - read value checking mask
   vExp=$(( 0 + $6 ))
   vMask=$(( 0 + $7 ))

   # Set the page offset
   i2cset -y $1 $2 $3 $4; fErrorMon $?
   # i2cget -y $1 $2 $5
   vRdVal=`i2cget -y $1 $2 $5 | awk '{ print $1 }'`; fErrorMon $?
   vLastRdVal=$vRdVal
   if [ $vMask -eq 0 ]; then
      printf "i2c_rdp $1 $2 ${4}_${5} $vRdVal [Exp=0x--] - No Check\n"
   else
      vRdValMask=$(( $vRdVal & $vMask ))
      vExpMask=$(( $vExp & $vMask ))
      if [ $vRdValMask -eq $vExpMask ]; then
         printf "i2c_rdp $1 $2 ${4}_${5} $vRdVal [Mask=0x%02x, MaskedVal=0x%02x] - Match\n" $vMask $vRdValMask
      else
         printf "i2c_rdp $1 $2 ${4}_${5} $vRdVal [Mask=0x%02x, MaskedVal=0x%02x, Exp=0x%02x, ExpMaskedVal=0x%02x] - Mismatch Error!\n" $vMask $vRdValMask $vExp $vExpMask
         fErrorMon 1
      fi
   fi
}


# List devices on all I2C buses
fListBusDevices () {
   for (( ii=0; ii<30; ii++ )); do
      ls -l /dev/i2c* | grep i2c-${ii} > /dev/null
      if [ $? -eq 0 ]; then
         echo "i2c-${ii} bus:"
         i2cdetect -y -r ${ii}
         echo ""
      fi
   done
}


# Find Si5382
fFindSi5382 () {
   printf "Scanning for Si5382... "
   for (( ii=0; ii<30; ii++ )); do
      #ls -l /dev/i2c* | grep i2c-${ii} > /dev/null
      #if [ $? -eq 0 ]; then
      if [ -e /dev/i2c-${ii} ]; then
         # Check for Si5382
         #echo "i2c-${ii} bus:"
         i2cdetect -y -r ${ii} | grep -i $vSi5382Addr > /dev/null
         if [ $? -eq 0 ]; then
            vSi5382Bus=${ii}
            vSi5382Addr="0x${vSi5382Addr}"
            echo "Detected Si5382 at bus=${vSi5382Bus}, address=${vSi5382Addr}."
            vSi5382Found=1
         fi
      fi
   done

   # Temporary hack to avoid invalid detection of device on bus 1
   if [ $vSi5382Bus -eq 1 ]; then
      echo "Warning - Si5382 unexpectedly detected on bus 1 - rescanning..."

      # Restore variable default
      vSi5382Found=0
      vSi5382Bus=99
      vSi5382Addr=68

      for (( ii=0; ii<30; ii++ )); do
         #ls -l /dev/i2c* | grep i2c-${ii} > /dev/null
         #if [ $? -eq 0 ]; then
         if [ -e /dev/i2c-${ii} ]; then
            # Check for Si5382
            #echo "i2c-${ii} bus:"
            i2cdetect -y -r ${ii} | grep -i $vSi5382Addr > /dev/null
            if [ $? -eq 0 ]; then
               vSi5382Bus=${ii}
               vSi5382Addr="0x${vSi5382Addr}"
               echo "Detected Si5382 at bus=${vSi5382Bus}, address=${vSi5382Addr}."
               vSi5382Found=1
            fi
         fi
      done
   fi

   if [ $vSi5382Found -eq 0 ]; then
      echo "Warning: Si5382 device not found!"
   fi
}


# Find SCI18IS602
fFindSCI18IS602 () {
   printf "Scanning for SCI18IS602... "
   for (( ii=0; ii<30; ii++ )); do
      #ls -l /dev/i2c* | grep i2c-${ii} > /dev/null
      #if [ $? -eq 0 ]; then
      if [ -e /dev/i2c-${ii} ]; then
         # Check for SCI18IS602
         #echo "i2c-${ii} bus:"
         i2cdetect -y -r ${ii} | grep -i $vSCI18IS602Addr > /dev/null
         if [ $? -eq 0 ]; then
            vSCI18IS602Bus=${ii}
            vSCI18IS602Addr="0x${vSCI18IS602Addr}"
            echo "Detected SCI18IS602 at bus=${vSCI18IS602Bus}, address=${vSCI18IS602Addr}."
            vSCI18IS602Found=1
         fi
      fi
   done
   if [ $vSCI18IS602Found -eq 0 ]; then
      echo "Warning: SCI18IS602 device not found!"
   fi
}

# Configure LMK04208
fCfgLMK04208 () {
	# Reference: https://manpages.debian.org/unstable/i2c-tools/i2cset.8.en.html
	# Scan bus to find SCI18IS602 I2C-to-SPI mux (assume LMK04208 is on this bus)
	fFindSCI18IS602

	if [ $vSCI18IS602Found -eq 1 ]; then
		# Configure SCI18IS602 operating mode
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0xf0 0x02; fErrorMon $?
		#i2cget -y $vSCI18IS602Bus $vSCI18IS602Addr 0xf0; fErrorMon $?

        # Load LMK registers (source LMK04208_ckin0=12M8_ck0_7m68_ck1_7m68_ck2_122m88_ck4_122m88.txt)
        # R0 	0x00160040
        # R0	0x00140300
        # R1	0x00140301
        # R2	0x00140302
        # R3	0x00140303
        # R4	0x00140304
        # R5	0x00140305
        # R6	0x01400006
        # R7	0x04400007
        # R8	0x08010008
        # R9	0x55555549
        # R10	0x9102440A
        # R11	0x0401100B
        # R12	0x130C006C
        # R13	0x2302820D
        # R14	0x0220000E
        # R15	0x8000800F
        # R16	0xC1550410
        # R24	0x00000058
        # R25	0x02C9C419
        # R26	0xAFA8001A
        # R27	0x1040009B
        # R28	0x0020099C
        # R29	0x0180019D
        # R30	0x0100019E
        # R31	0x003F001F
        printf "Configuring LMK04208 output for 122.88 MHz...\n"
        i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr 0x00 0x16 0x00 0x40 i; fErrorMon $?
        i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr 0x00 0x14 0x03 0x00 i; fErrorMon $?
        i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr 0x00 0x14 0x03 0x01 i; fErrorMon $?
        i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr 0x00 0x14 0x03 0x02 i; fErrorMon $?
        i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr 0x00 0x14 0x03 0x03 i; fErrorMon $?
        i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr 0x00 0x14 0x03 0x04 i; fErrorMon $?
        i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr 0x00 0x14 0x03 0x05 i; fErrorMon $?
        i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr 0x01 0x40 0x00 0x06 i; fErrorMon $?
        i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr 0x04 0x40 0x00 0x07 i; fErrorMon $?
        i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr 0x08 0x01 0x00 0x08 i; fErrorMon $?
        i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr 0x55 0x55 0x55 0x49 i; fErrorMon $?
        i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr 0x91 0x02 0x44 0x0A i; fErrorMon $?
        i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr 0x04 0x01 0x10 0x0B i; fErrorMon $?
        i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr 0x13 0x0C 0x00 0x6C i; fErrorMon $?
        i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr 0x23 0x02 0x82 0x0D i; fErrorMon $?
        i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr 0x02 0x20 0x00 0x0E i; fErrorMon $?
        i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr 0x80 0x00 0x80 0x0F i; fErrorMon $?
        i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr 0xC1 0x55 0x04 0x10 i; fErrorMon $?
        i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr 0x00 0x00 0x00 0x58 i; fErrorMon $?
        i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr 0x02 0xC9 0xC4 0x19 i; fErrorMon $?
        i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr 0xAF 0xA8 0x00 0x1A i; fErrorMon $?
        i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr 0x10 0x40 0x00 0x9B i; fErrorMon $?
        i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr 0x00 0x20 0x09 0x9C i; fErrorMon $?
        i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr 0x01 0x80 0x01 0x9D i; fErrorMon $?
        i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr 0x01 0x00 0x01 0x9E i; fErrorMon $?
        i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr 0x00 0x3F 0x00 0x1F i; fErrorMon $?
	fi
}

# Configure LMK04208
fCfgLMX2594 () {
	# Reference: https://manpages.debian.org/unstable/i2c-tools/i2cset.8.en.html
    # Scan bus to find SCI18IS602 I2C-to-SPI mux (assume LMX2594 is on this bus)
	fFindSCI18IS602

	if [ $vSCI18IS602Found -eq 1 ]; then
		printf "Configuring LMX2594_A\n"

# R112    0x700000
# R111    0x6F0000
# R110    0x6E0000
# R109    0x6D0000
# R108    0x6C0000
# R107    0x6B0000
# R106    0x6A0000
# R105    0x690021
# R104    0x680000
# R103    0x670000
# R102    0x663F80
# R101    0x650011
# R100    0x640000
# R99     0x630000
# R98     0x620200
# R97     0x610888
# R96     0x600000
# R95     0x5F0000
# R94     0x5E0000
# R93     0x5D0000
# R92     0x5C0000
# R91     0x5B0000
# R90     0x5A0000
# R89     0x590000
# R88     0x580000
# R87     0x570000
# R86     0x560000
# R85     0x55D300
# R84     0x540001
# R83     0x530000
# R82     0x521E00
# R81     0x510000
# R80     0x506666
# R79     0x4F0026
# R78     0x4E0003
# R77     0x4D0000
# R76     0x4C000C
# R75     0x4B0980
# R74     0x4A0000
# R73     0x49003F
# R72     0x480001
# R71     0x470081
# R70     0x46C350
# R69     0x450000
# R68     0x4403E8
# R67     0x430000
# R66     0x4201F4
# R65     0x410000
# R64     0x401388
# R63     0x3F0000
# R62     0x3E0322
# R61     0x3D00A8
# R60     0x3C0000
# R59     0x3B0001
# R58     0x3A8001
# R57     0x390020
# R56     0x380000
# R55     0x370000
# R54     0x360000
# R53     0x350000
# R52     0x340820
# R51     0x330080
# R50     0x320000
# R49     0x314180
# R48     0x300300
# R47     0x2F0300
# R46     0x2E07FC
# R45     0x2DC0CC
# R44     0x2C0C23
# R43     0x2B0000
# R42     0x2A0000
# R41     0x290000
# R40     0x280000
# R39     0x270001
# R38     0x260000
# R37     0x250304
# R36     0x240050
# R35     0x230004
# R34     0x220000
# R33     0x211E21
# R32     0x200393
# R31     0x1F43EC
# R30     0x1E318C
# R29     0x1D318C
# R28     0x1C0488
# R27     0x1B0002
# R26     0x1A0DB0
# R25     0x190624
# R24     0x18071A
# R23     0x17007C
# R22     0x160001
# R21     0x150401
# R20     0x14E048
# R19     0x1327B7
# R18     0x120064
# R17     0x11012C
# R16     0x100080
# R15     0x0F064F
# R14     0x0E1E70
# R13     0x0D4000
# R12     0x0C5001
# R11     0x0B0018
# R10     0x0A10D8
# R9      0x090604
# R8      0x082000
# R7      0x0740B2
# R6      0x06C802
# R5      0x0500C8
# R4      0x040A43
# R3      0x030642
# R2      0x020500
# R1      0x010808
# R0      0x00249C
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x70 0x00 0x00 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x6F 0x00 0x00 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x6E 0x00 0x00 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x6D 0x00 0x00 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x6C 0x00 0x00 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x6B 0x00 0x00 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x6A 0x00 0x00 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x69 0x00 0x21 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x68 0x00 0x00 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x67 0x00 0x00 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x66 0x3F 0x80 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x65 0x00 0x11 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x64 0x00 0x00 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x63 0x00 0x00 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x62 0x02 0x00 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x61 0x08 0x88 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x60 0x00 0x00 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x5F 0x00 0x00 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x5E 0x00 0x00 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x5D 0x00 0x00 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x5C 0x00 0x00 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x5B 0x00 0x00 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x5A 0x00 0x00 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x59 0x00 0x00 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x58 0x00 0x00 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x57 0x00 0x00 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x56 0x00 0x00 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x55 0xD3 0x00 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x54 0x00 0x01 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x53 0x00 0x00 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x52 0x1E 0x00 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x51 0x00 0x00 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x50 0x66 0x66 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x4F 0x00 0x26 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x4E 0x00 0x03 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x4D 0x00 0x00 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x4C 0x00 0x0C i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x4B 0x09 0x80 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x4A 0x00 0x00 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x49 0x00 0x3F i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x48 0x00 0x01 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x47 0x00 0x81 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x46 0xC3 0x50 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x45 0x00 0x00 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x44 0x03 0xE8 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x43 0x00 0x00 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x42 0x01 0xF4 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x41 0x00 0x00 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x40 0x13 0x88 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x3F 0x00 0x00 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x3E 0x03 0x22 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x3D 0x00 0xA8 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x3C 0x00 0x00 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x3B 0x00 0x01 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x3A 0x80 0x01 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x39 0x00 0x20 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x38 0x00 0x00 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x37 0x00 0x00 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x36 0x00 0x00 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x35 0x00 0x00 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x34 0x08 0x20 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x33 0x00 0x80 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x32 0x00 0x00 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x31 0x41 0x80 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x30 0x03 0x00 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x2F 0x03 0x00 i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x2E 0x07 0xFC i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x2D 0xC0 0xCC i
		i2cset -y $vSCI18IS602Bus $vSCI18IS602Addr 0x8 0x2C 0x0C 0x23 i
